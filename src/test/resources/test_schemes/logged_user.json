{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "type": {
      "type": "string"
    },
    "accessToken": {
      "type": "string"
    },
    "refreshToken": {
      "type": "string"
    },
    "userDTO": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "surname": {
          "type": "string"
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "roles": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "id",
        "name",
        "surname",
        "email",
        "roles"
      ]
    }
  },
  "required": [
    "type",
    "accessToken",
    "refreshToken",
    "userDTO"
  ]
}