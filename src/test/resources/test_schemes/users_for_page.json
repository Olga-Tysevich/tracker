{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "usersForPage": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer" },
          "name": { "type": "string" },
          "surname": { "type": "string" },
          "email": { "type": "string" },
          "roles": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["id", "name", "surname", "email", "roles"]
      }
    },
    "totalItems": {
      "type": "integer"
    }
  },
  "required": ["usersForPage", "totalItems"]
}